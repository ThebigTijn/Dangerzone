    set name of last spawned entity to "096"
    set {_096} to last spawned entity
    apply invisibility without particles to {_096} for 1 year
    while {_096} is alive:
        wait 1 tick
        set {_tp} to {_096}'s location
        set pitch of {_tp} to 0
        if {_w} is not set:
