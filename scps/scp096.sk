function walk(l: location):
    if {w} is not set:
        set {w} to true
        teleport {model::096FLFR::*} 1 block above {_l}
        wait 0.2 seconds
        teleport {model::096HFHR::*} 1 block above {_l}
        wait 0.2 seconds
        teleport {model::096BASE::*} 1 block above {_l}
        wait 0.2 seconds
        teleport {model::096HRHL::*} 1 block above {_l}
        wait 0.2 seconds
        teleport {model::096FRFL::*} 1 block above {_l}
        wait 0.2 seconds
        delete {w}


