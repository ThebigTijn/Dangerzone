function shopPurchase(p: player, item: item, num: num):

    if moneyGet({_p}) >= {_num}:

        send "<##E34234>&lSYSTEM &7» Purchased &f%name of {_item}%&7, costed &a$%{_num}%" to {_p}
        moneyRemove({_p}, {_num})
        give {_p} {_item}
    
    else:

        send "<##E34234>&lSYSTEM &7» Insufficient Funds" to {_p}